{
    "require": {
        "artyomderyavka/fast-micro-kernel": "dev-master",
        "artyomderyavka/target": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Delivery\\Controllers\\" : "application/controllers/",
            "Delivery\\Factories\\" : "application/factories/",
            "Delivery\\Factories\\Interfaces\\" : "application/factories/interfaces",
            "Delivery\\Dto\\" : "application/dto/",
            "Delivery\\Domain\\DeliveryContext\\Services\\" : "domain/deliveryContext/services/",
            "Delivery\\Domain\\DeliveryContext\\Services\\Interfaces\\" : "domain/deliveryContext/services/interfaces",
            "Delivery\\Domain\\DeliveryContext\\Dto\\" : "domain/deliveryContext/dto/",
            "Delivery\\Domain\\DeliveryContext\\Dto\\Interfaces\\" : "domain/deliveryContext/dto/interfaces/",
            "Delivery\\Domain\\DeliveryContext\\Factories\\" : "domain/deliveryContext/factories/",
            "Delivery\\Domain\\DeliveryContext\\Factories\\Interfaces\\" : "domain/deliveryContext/factories/interfaces/",
            "Delivery\\Domain\\DeliveryContext\\Collections\\" : "domain/deliveryContext/collections/",
            "Delivery\\Domain\\DeliveryContext\\Collections\\Interfaces\\" : "domain/deliveryContext/collections/interfaces/",
            "Delivery\\Domain\\DeliveryContext\\ValueObjects\\" : "domain/deliveryContext/valueObjects/",
            "Delivery\\Domain\\DeliveryContext\\ValueObjects\\Interfaces\\" : "domain/deliveryContext/valueObjects/interfaces/"
        }
    },
    "classmap": [
        "application/controllers",
        "application/factories",
        "application/factories/interfaces",
        "application/dto",
        "domain/deliveryContext/services",
        "domain/deliveryContext/services/interfaces",
        "domain/deliveryContext/dto",
        "domain/deliveryContext/dto/interfaces",
        "domain/deliveryContext/factories",
        "domain/deliveryContext/factories/interfaces",
        "domain/deliveryContext/collections",
        "domain/deliveryContext/collections/interfaces",
        "domain/deliveryContext/valueObjects",
        "domain/deliveryContext/valueObjects/interfaces"
    ],
    "minimum-stability": "dev"
}
